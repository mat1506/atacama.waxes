@article{pollardBuildingLipidBarriers2008,
  title = {Building Lipid Barriers: Biosynthesis of Cutin and Suberin},
  shorttitle = {Building Lipid Barriers},
  author = {Pollard, Mike and Beisson, Fred and Li, Yonghua and Ohlrogge, John B.},
  year = {2008},
  month = may,
  journal = {Trends in Plant Science},
  volume = {13},
  number = {5},
  pages = {236--246},
  issn = {1360-1385},
  doi = {10.1016/j.tplants.2008.03.003},
  abstract = {Cutin and suberin are the polymer matrices for lipophilic cell wall barriers. These barriers control the fluxes of gases, water and solutes, and also play roles in protecting plants from biotic and abiotic stresses and in controlling plant morphology. Although they are ubiquitous, cutin and suberin are the least understood of the major plant extracellular polymers. The use of forward and reverse genetic approaches in Arabidopsis has led to the identification of oxidoreductase and acyltransferase genes involved in the biosynthesis of these polymers. However, major questions about the underlying polymer structure, biochemistry, and intracellular versus extracellular assembly remain to be resolved. The analysis of plant lines with modified cutins and suberins has begun to reveal the inter-relationships between the composition and function of these polymers.},
  langid = {english},
  file = {/home/mat15063/snap/zotero-snap/common/Zotero/storage/38XVIUBX/S1360138508001076.html}
}

@article{quadeCentralAndean282022,
  title = {Central {{Andean}} (28\textendash 34\textdegree{{S}}) Flood Record 0\textendash 25 Ka from {{Salinas}} Del {{Bebedero}}, {{Argentina}}},
  author = {Quade, Jay and Dente, Elad and Cartwright, Alyson and Hudson, Adam and {Jimenez-Rodriguez}, Sebastian and McGee, David},
  year = {2022},
  month = apr,
  journal = {Quaternary Research},
  pages = {1--27},
  publisher = {{Cambridge University Press}},
  issn = {0033-5894, 1096-0287},
  doi = {10.1017/qua.2022.1},
  abstract = {The Salinas del Bebedero occupies an isolated basin in the foreland of central Argentina at 33\textdegree S and was flooded repeatedly over past 25 ka. Isotopic evidence demonstrates that this flooding was due to overflow of the nearby R\'io Desaguadero with waters derived from the distant ({$\geq$}300 km) central Andes between 28\textendash 34\textdegree S. Stratigraphic and shoreline evidence shows that floods occurred most frequently between 14.4 and 15.7 ka, followed by lesser events between 14.3 to 11.4 ka, and during the late Holocene from 2.6 to ca. 0.2 ka. Hydraulic modeling (2D HEC-RAS) shows that these floods could have originated from repeated subglacial drainage or sudden outbursts with a volume of {$>$}100 \texttimes{} 106 m3 and a peak discharge of {$>$}1,000 m3 s-1 each. The absence of flood deposits from 11 to 3 ka points to exceptionally dry and virtually ice-free conditions in the Andes between 28\textendash 34\textdegree S. The floods were probably caused by major rainfall or dammed-lake outbursts clustered largely during wet pluvial periods in the otherwise moisture-limited central Andes and Atacama Desert, such as when the Intertropical Convergence Zone was shifted southward. These include Central Andean pluvial events (CAPE) I (17\textendash 14.5 ka) and II (12.5\textendash 9 ka), and the Neoglacial/Formative archeological period 2500 ka to near-present.},
  langid = {english},
  keywords = {Bebedero,Central Andes,Floods,Oxygen isotopes},
  file = {/home/mat15063/snap/zotero-snap/common/Zotero/storage/K7XRLCUV/Quade et al. - 2022 - Central Andean (28–34°S) flood record 0–25 ka from.pdf;/home/mat15063/snap/zotero-snap/common/Zotero/storage/YGVTJPJE/C7AB30332AC66C12988887E5C5884639.html}
}

@article{quadePaleowetlandsRegionalClimate2008,
  title = {Paleowetlands and Regional Climate Change in the Central {{Atacama Desert}}, Northern Chile},
  author = {Quade, Jay and Rech, Jason A. and Betancourt, Julio L. and Latorre, Claudio and Quade, Barbra and Rylander, Kate Aasen and Fisher, Timothy},
  year = {2008},
  month = mar,
  journal = {Quaternary Research},
  volume = {69},
  number = {3},
  pages = {343--360},
  publisher = {{Cambridge University Press}},
  issn = {0033-5894, 1096-0287},
  doi = {10.1016/j.yqres.2008.01.003},
  abstract = {Widespread, organic-rich diatomaceous deposits are evidence for formerly wetter times along the margins of the central Atacama Desert, one of the driest places on Earth today. We mapped and dated these paleowetland deposits at three presently waterless locations near Salar de Punta Negra (24.5\textdegree S) on the western slope of the Andes. Elevated groundwater levels supported phreatic discharge into wetlands during two periods: 15,900 to \textasciitilde{} 13,800 and 12,700 to \textasciitilde{} 9700 cal yr BP. Dense concentrations of lithic artifacts testify to the presence of paleoindians around the wetlands late in the second wet phase (11,000?\textendash 9700 cal yr BP). Water tables dropped below the surface before 15,900 and since 8100 cal yr BP, and briefly between \textasciitilde{} 13,800 and 12,700 cal yr BP. This temporal pattern is repeated, with some slight differences, in rodent middens from the study area, in both paleowetland and rodent midden deposits north and south of the study area, and in lake level fluctuations on the adjacent Bolivian Altiplano. The regional synchroneity of these changes points to a strengthening of the South American Monsoon \textemdash{} which we term the "Central Andean Pluvial Event" \textemdash{} in two distinct intervals (15,900\textendash 13,800 and 12,700\textendash 9700 cal yr BP), probably induced by steepened SST gradients across the tropical Pacific (i.e., La Ni\~na-like conditions).},
  langid = {english},
  keywords = {Atacama,Carbon-14,El Niño,ENSO,La Niña,Paleowetlands},
  file = {/home/mat15063/snap/zotero-snap/common/Zotero/storage/2KRZ2IFB/Quade et al. - 2008 - Paleowetlands and regional climate change in the c.pdf}
}

@article{rabatelChronologyLittleIce2008b,
  title = {A Chronology of the {{Little Ice Age}} in the Tropical {{Andes}} of {{Bolivia}} (16\textdegree{{S}}) and Its Implications for Climate Reconstruction},
  author = {Rabatel, Antoine and Francou, Bernard and Jomelli, Vincent and Naveau, Philippe and Grancher, Delphine},
  year = {2008},
  month = sep,
  journal = {Quaternary Research},
  volume = {70},
  number = {2},
  pages = {198--212},
  issn = {0033-5894},
  doi = {10.1016/j.yqres.2008.02.012},
  abstract = {Dating moraines by lichenometry enabled us to reconstruct glacier recession in the Bolivian Andes since the Little Ice Age maximum. On the 15 proglacial margins studied, we identified a system of ten principal moraines that marks the successive positions of glaciers over the last four centuries. Moraines were dated by performing statistical analysis of lichen measurements based on the extreme values theory. Like glaciers in many mid-latitude mountain areas, Bolivian glaciers reached their maximal extent during the second half of the 17th century. This glacier maximum coincides with the Maunder minimum of solar irradiance. By reconstructing the equilibrium-line altitude and changes in mass-balance, we think the glacier maximum may be due to a 20 to 30\% increase in precipitation and a 1.1 to 1.2~\textdegree C decrease in temperature compared with present conditions. In the early 18th century, glaciers started to retreat at varying rates until the late 19th to early 20th century; this trend was generally associated with decreasing accumulation rates. By contrast, glacier recession in the 20th century was mainly the consequence of an increase in temperature and humidity. These results are consistent with observations made in the study region based on other proxies.},
  keywords = {Bolivia,Climate reconstruction,Glacier fluctuations,Lichenometry,Little ice age,Moraines,Tropical Andes}
}

@article{rabatelDatingLittleIce2005,
  title = {Dating of {{Little Ice Age}} Glacier Fluctuations in the Tropical {{Andes}}: {{Charquini}} Glaciers, {{Bolivia}}, 16\textdegree{{S}}},
  shorttitle = {Dating of {{Little Ice Age}} Glacier Fluctuations in the Tropical {{Andes}}},
  author = {Rabatel, Antoine and Jomelli, Vincent and Naveau, Philippe and Francou, Bernard and Grancher, Delphine},
  year = {2005},
  month = nov,
  journal = {Comptes Rendus Geoscience},
  volume = {337},
  number = {15},
  pages = {1311--1322},
  issn = {1631-0713},
  doi = {10.1016/j.crte.2005.07.009},
  abstract = {Fluctuations of the Charquini glaciers (Cordillera Real, Bolivia) have been reconstructed for the Little Ice Age (LIA) from a set of 10 moraines extending below the present glacier termini. A lichenometric method using the Rhizocarpon geographicum was used to date the moraines and reconstruct the main glacier fluctuations over the period. The maximum glacier extent occurred in the second half of the 17th century, followed by nearly continuous retreat with three interruptions during the 18th and the 19th centuries, marked by stabilisation or minor advances. Results obtained in the Charquini area are first compared with other dating performed in the Peruvian Cordillera Blanca and then with the fluctuations of documented glaciers in the Northern Hemisphere. Glacier fluctuations along the tropical Andes (Bolivia and Peru) were in phase during the LIA and the solar forcing appears to be important during the period of glacier advance. Compared with the Northern Hemisphere mid-latitudes, the major advance observed on these glaciers during the first half of the 19th century is not present in the tropical Andes. This discrepancy may be due to regional scale climate variations. To cite this article: A. Rabatel et al., C. R. Geoscience 337 (2005).},
  keywords = {Andes,Bolivia,Bolivie,Glacier,Glaciers,Lichénométrie,Lichenometry,Little ice age,Moraine,Moraines,Petit âge glaciaire}
}

@article{ramOptimizedAcceleratedSolvent2022,
  title = {An Optimized Accelerated Solvent Extraction and Purification Method for Enhanced Recovery of N-Alkanes (Notably Short-Chains) from Environmental Samples Using the Conventional Dichloromethane/Methanol Solvent},
  author = {Ram, Fulmati and Kumar Bera, Melinda and Sarkar, Anindya},
  year = {2022},
  month = jan,
  journal = {Organic Geochemistry},
  pages = {104368},
  issn = {0146-6380},
  doi = {10.1016/j.orggeochem.2022.104368},
  abstract = {Sedimentary lipid biomarker proxies are extensively used for paleoclimatic and paleoenvironmental research. Among all the lipid compounds, the non-polar n-alkanes (chain lengths n-C14 to n-C37) are one of the most widely used lipid fractions for such studies. However, the lack of optimized automated extraction and post-extraction sample workup procedure, using conventional polar solvent, from environmental samples, has been challenging. Conversely, extraction of the n-alkanes using non-polar solvents restricts the co-extraction of other polar lipid compounds like ketones, alcohols, acids, etc. which may be of interest to many researchers. Here we report an optimized total lipid extraction method using the conventional polar dichloromethane/methanol solvents with standardized Automated Solvent Extractor (ASE) parameters and post-extraction sample work-up protocols. The robustness of the method has been tested by extracting eight samples containing algae, leaf, and sediment/rock of varying age (spanning from recent to {$\sim$}252 Ma). The results of the extraction efficiency (yield) of the method were compared with commonly used methods, those using either non-polar n-hexane or polar dichloromethane/methanol solvents. For n-alkane extraction from algal biomass, the performance of the proposed method and the conventional polar solvent method are statistically comparable, but the new method performs better than the non-polar solvent method. Likewise, for the leaf and sediment samples, the performance of the proposed method is equivalent to the non-polar solvent method but outperforms the commonly used polar solvent method. The Terrestrial Aquatic Ratio (TAR) and long-chain hydrocarbon/short-chain hydrocarbon (LHC/SHC) ratios calculated from the algae and sediment samples also show that our recommended extraction approach can be confidently used in calculating n-alkane ratio proxies, which critically depend on the extraction efficiency of the short-chain n-alkanes Taken together, the new method will be highly useful for studying the n-alkanes from a wide spectrum of natural sample matrices and provide reliable data for paleoclimatic inference.},
  langid = {english},
  keywords = {-alkanes,-C-C; Recovery,Accelerated Solvent Extraction,Algae,Leaf,Lipids,Sediments,Short-chain}
}

@article{rechLateQuaternaryPaleohydrology2002,
  title = {Late {{Quaternary}} Paleohydrology of the Central {{Atacama Desert}} (Lat 22\textdegree\textendash 24\textdegree{{S}}), {{Chile}}},
  author = {Rech, Jason A. and Quade, Jay and Betancourt, Julio L.},
  year = {2002},
