@article{nieto-morenoElevationdependentChangesNalkane2016,
  title = {Elevation-Dependent Changes in n-Alkane {{$\delta$D}} and Soil {{GDGTs}} across the {{South Central Andes}}},
  author = {{Nieto-Moreno}, Vanesa and Rohrmann, Alexander and {van der Meer}, Marcel T. J. and Sinninghe Damst{\'e}, Jaap S. and Sachse, Dirk and Tofelde, Stefanie and Niedermeyer, Eva M. and Strecker, Manfred R. and Mulch, Andreas},
